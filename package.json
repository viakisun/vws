{
  "name": "vws",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src/ --ext .svelte,.ts --no-fix",
    "lint:fix": "echo '⚠️ 반응성 코드는 수동으로 수정하세요' && exit 1",
    "quality": "node scripts/quality-check.js",
    "quality:ci": "npm run check && npm run lint && npm run quality && npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "audit": "npm audit",
    "outdated": "npm outdated"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-plugin-svelte": "^3.12.4",
    "svelte": "^5.39.6",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.0.0"
  },
  "type": "module",
  "dependencies": {
    "@lucide/svelte": "^0.544.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@tailwindcss/postcss": "^4.1.13",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  }
}
