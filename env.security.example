# Excel 파일 보안 설정
# 파일 크기 제한 (바이트) - 기본 10MB
EXCEL_MAX_FILE_SIZE=10485760

# 최대 행 수 제한 - 기본 10,000행
EXCEL_MAX_ROWS=10000

# 최대 컬럼 수 제한 - 기본 100컬럼
EXCEL_MAX_COLS=100

# 파싱 타임아웃 (밀리초) - 기본 30초
EXCEL_PARSE_TIMEOUT=30000

# 보안 레벨 (strict|moderate|lenient)
# strict: 매크로 발견 시 파일 거부
# moderate: 매크로 발견 시 경고만 표시
# lenient: 매크로 무시
EXCEL_SECURITY_LEVEL=strict

# 로그 레벨 (debug|info|warn|error)
EXCEL_LOG_LEVEL=info

# 임시 파일 삭제 여부
EXCEL_CLEANUP_TEMP_FILES=true

# 격리 실행 여부 (향후 구현)
EXCEL_ISOLATED_PARSING=false
